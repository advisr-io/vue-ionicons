{
  "name": "vue-ionicons",
  "version": "3.0.2",
  "license": "MIT",
  "author": "Irfan Maulana (https://github.com/mazipan/)",
  "description": "Vue Icon Set Components from Ionic Team",
  "scripts": {
    "dev": "rimraf demo/dist && cross-env NODE_ENV=development ./node_modules/.bin/poi --serve",
    "build": "rimraf demo/dist && cross-env NODE_ENV=production ./node_modules/.bin/poi --prod",
    "dist-lib": "rimraf dist/js && cross-env NODE_ENV=production ./node_modules/.bin/rollup -c",
    "dist": "node dist.js && npm run dist-lib",
    "deploy": "node ./gh-publish.js"
  },
  "files": [
    "dist/**",
    "ionicons.css",
    "ionicons.scss",
    "ionicons.less"
  ],
  "keywords": [
    "vue ionicons",
    "vue icon",
    "vue font icon",
    "ionicons"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.11.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-vue": "5.1.0",
    "file-loader": "^3.0.1",
    "gh-pages": "2.0.1",
    "mustache": "^3.0.1",
    "node-sass": "^4.11.0",
    "ora": "^3.0.0",
    "poi": "12.2.14",
    "rimraf": "^2.6.3",
    "rollup": "^1.0.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "style-loader": "0.23.1",
    "svgo": "^1.1.1",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/mazipan/vue-ionicons.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/vue-ionicons/issues"
  }
}
